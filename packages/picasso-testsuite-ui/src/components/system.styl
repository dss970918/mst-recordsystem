
body
  margin: 0;
  #system-root
    .guide
      width: 100%;
      p
        cursor: pointer;
        margin-left: 50px;
      .top-container
        width: 100%;
        background-color: #00A3FF;
        height:100px;
        display: flex;
        justify-content: flex-start;
        align-items: center;
      .main-container
        .function-p
          p
          color: #00A3FF;
        .foot
          div
            margin-left: 50px;
          span
            color: #00A3FF;
            cursor: pointer;

    .page-header
      border-bottom: 1px solid rgba(180, 180, 180, 1);
      height: 50px;
      .title
        font-weight: bold;
        width: 200px;
        margin-left: 10px;
        padding-top: 15px;
        display: inline-block;
      .icon-list
        width: 150px;
        padding-top: 10px;
        display: inline-block;
        float: right;
        .icon
          svg
            width: 20px;
            height: 20px; 
    .main-page
      position: absolute;
      width: 90%;
      right:0;
      .icons-list
        margin-top: 10px;
        display: inline-block;
        .icon
          svg
            width: 20px;
            height: 20px; 
      .icons-list-right
        width: 150px;
        margin-top: 10px;
        float: right;
        display: inline-block;
        .icon
          svg
            width: 20px;
            height: 20px; 

      .top-url
        margin: 0;
      .input
        div
          .span-name
            width: 30px;
            display: inline-block;

    .testcase
      width: 9%;
      .top-select
        display: flex;
        align-items: center;
        justify-content: space-between;
    .menubar
      position: absolute;
      bottom: 20%;
      width: 100%;
      .icons-list-right
        width: 30px;
        margin-top: 10px;
        display: inline-block;
        position: absolute;
        right:0;
        bottom: 0;
        .icon
          svg
            width: 20px;
            height: 20px; 



        
        
        



        








